<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= will_paginate %>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user, size: 50 %>
      <%= link_to user.name, user %>
      <% if logged_in? %>
        <h1> HI
          <%= user.logged %>
        </h1>
      <% end %>
    </li>
  <% end %>
</ul>

<%= will_paginate %>